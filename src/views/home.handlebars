<div>
  <h1>Lista de productos </h1>
  <table>
    <thead>
      <tr>
        <td>Nombre</td>
        <td>Descripción</td>
        <td>Precio</td>
        <td>Imagen</td>
        <td>Stock</td>
        <td>Categoría</td>
      </tr>
    </thead>
    <tbody>
      {{#each products}}
        <tr>
          <td> {{this.title}} </td>
          <td> {{this.description}} </td>
          <td> {{this.price}} </td>
          <td><img src={{this.thumbnail}} /></td>
          <td> {{this.stock}} </td>
          <td> {{this.category}} </td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>